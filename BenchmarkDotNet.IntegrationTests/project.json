{
  "title": "BenchmarkDotNet.IntegrationTests",
  "commands": {
    "test": "xunit.runner.dnx"
  },
  "compileExclude": "**/Program.cs",
  "veryImportantComment": "we need to exclude all auto-generated files that created during integration test execution",
  "frameworks": {
    "net45": {
      "dependencies": {
        "Dia2Lib": "1.0.0-*",
        "FSharpBenchmarkDotNet": "1.0.0-*",
        "Interop.Dia2Lib": "1.0.0-*",
        "Microsoft.Diagnostics.Runtime": "1.0.0-*",
        "xunit.runner.visualstudio": "2.1.0"
      },
      "frameworkAssemblies": {
        "System.Runtime": "4.0.0.0",
        "System.Threading.Tasks": "4.0.0.0"
      }
    },
    "dnx451": {
      "frameworkAssemblies": {
        "System.Runtime": "4.0.10.0",
        "System.Threading.Tasks": "4.0.0.0"
      },
      "dependencies": {
        "Dia2Lib": "1.0.0-*",
        "FSharpBenchmarkDotNet": "1.0.0-*",
        "Interop.Dia2Lib": "1.0.0-*",
        "Microsoft.Diagnostics.Runtime": "1.0.0-*",
        "xunit.runner.dnx": "2.1.0-rc1-build204"
      }
    }
  },
  "dependencies": {
    "BenchmarkDotNet": {
      "target": "project",
      "version": "0.8.1"
    },
    "BenchmarkDotNet.Diagnostics": "1.0.0-*",
    "xunit": "2.1.0",
    "xunit.abstractions": "2.0.0",
    "xunit.assert": "2.1.0",
    "xunit.core": "2.1.0",
    "xunit.extensibility.core": "2.1.0",
    "xunit.extensibility.execution": "2.1.0"
  }
}